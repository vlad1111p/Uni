package ub2a2;
import ub2a2.Zellraum;
import ub2a2.Nachbarschaft.Pair;

public class Nachbarschaft {

	Nachbarschaft() {

	}

	public void setMoore() {

	}

	public void setVonNeumann() {

	}
	
	


	public class Pair<A, B> {
		public final A fst;
		public final B snd;

		public Pair(final A fst, final B snd) {
			this.fst = fst;
			this.snd = snd;
		}
	}
}
